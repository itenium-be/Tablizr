{
    "manifest_version": 2,
    "name": "Tablizr",
    "description": "Turn any html table into a powerhouse",
    "version": "0.0.1",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_title": "Tablize current page"
    },
    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Alt+T",
          "windows": "Alt+T",
          "mac": "Alt+T",
          "linux": "Alt+T"
        }
      }
    },
    "permissions": ["storage", "unlimitedStorage", "contextMenus", "tabs", "http://*/*", "https://*/*", "file:///*"]
}
